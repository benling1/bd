<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
                   http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
	
	
	
	<bean id="skipPayConfig" class="com.kycrm.member.utils.pay.SkipPayConfig">
		<property name="app_id">
			<value>2017110609762911</value> 
		</property>
		<property name="merchant_private_key">
			<value></value> 
		</property>
		<property name="alipay_public_key">
			<value></value> 
		</property>
		<property name="notify_url">
			<value>${notify}</value>
		</property>
		<property name="return_url">
			<value></value> 
		</property>
		<property name="sign_type">
			<value>RSA2</value> 
		</property>
		<property name="charset">
			<value>utf-8</value> 
		</property>
		<property name="gatewayUrl">
			<value>https://openapi.alipay.com/gateway.do</value> 
		</property>
	</bean>
	
</beans>               
